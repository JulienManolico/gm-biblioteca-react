# Configurações do Supabase
# Copie este arquivo para .env.local e preencha com suas credenciais

# URL do seu projeto Supabase
REACT_APP_SUPABASE_URL=https://seu-projeto.supabase.co

# Chave anônima do Supabase (pública)
REACT_APP_SUPABASE_ANON_KEY=sua-chave-anonima-aqui

# Configurações da aplicação
REACT_APP_APP_NAME=GMBiblioteca
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# URL da API (se usar backend separado)
REACT_APP_API_URL=mock
